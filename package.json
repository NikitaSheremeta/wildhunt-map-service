{
  "name": "wldhunt-map-service",
  "version": "1.0.0",
  "description": "Map service",
  "main": "src/api/v1/index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node src/api/v1/index.js",
    "serve": "cross-env NODE_ENV=development npm run watch-node",
    "watch-node": "nodemon",
    "eslint": "eslint --fix './**/*.js'",
    "prettier": "prettier --write './**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikitaSheremeta/wildhut-map-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NikitaSheremeta/wildhut-map-service/issues"
  },
  "homepage": "https://github.com/NikitaSheremeta/wildhut-map-service#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.5",
    "cross-env": "^7.0.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "8.22.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "helmet": "^6.1.5"
  }
}
